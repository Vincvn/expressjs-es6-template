{
  "name": "expressjs-es6-template",
  "version": "1.0.0",
  "private": true,
  "description": "ExpressJS ES6 Template",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.js --exec babel-node",
    "build": "babel src -d dist --copy-files",
    "postinstall": "npm run build"
  },
  "author": "Vincent Hoang",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^2.4.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
